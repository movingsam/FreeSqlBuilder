@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
    var builder = Model.CurrentBuilder;
    var dtoName =  project.GetBuilder("Dtos").GetName(table.CsName);
    var primaryKeyType = table.GetPrimarysName();
    var className = builder.GetName(table.CsName);
}

@table.GetNameSpaceUsing();

namespace @(project.ProjectInfo.ProjectName).@className
{
    public class @className : PageViewDtoBase<@($"{dtoName},{primaryKeyType}")>
    {
       public @className (IPaged paged, List<@dtoName> datas):base(paged,datas)
       {
        
       }
    }
}