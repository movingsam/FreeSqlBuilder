@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
    var builder = Model.CurrentBuilder;

}
using @table.Type.Namespace;
@table.GetNameSpaceUsing()

namespace @(project.ProjectInfo.ProjectName).@project.GetBuilder("Dtos").Name
{
    @($"{table.Comment}Dtos".GetCSharpSummary())
    public class @builder.GetName(table.CsName) : DtoBase<@table.GetPrimarysName()>
    {
        @foreach (var column in table.Columns)
        {
        @if (column.Key == "Id")
         {
             continue;
         }
        @(column.Value.Comment.GetCSharpSummary())
        @Html.NewLine()
        @:public  @column.Value.CsType.Name @column.Value.CsName { get; set; }
        @Html.NewLine()
        }
    }
}