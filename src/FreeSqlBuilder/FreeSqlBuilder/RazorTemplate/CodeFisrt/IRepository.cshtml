@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
    var builder = Model.CurrentBuilder;
}

using System;
using System.Collections.Generic;
using System.Text;

namespace @(project.ProjectInfo.ProjectName).Abstractes.Repositories
{

    public interface @(builder.GetName(table.CsName)) :IURepository<@(table.CsName)@(",")I@(project.ProjectInfo.ProjectName)DbContext>
    {
    }
}
