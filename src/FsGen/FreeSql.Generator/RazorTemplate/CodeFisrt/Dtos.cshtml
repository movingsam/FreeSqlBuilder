@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
    
}
using @table.NameSpace
@table.GetNameSpaceUsing()

namespace @(project.ProjectInfo.ProjectName).@project.GetBuilder("Dtos").Name
{
    @($"{table.Comment}Dtos".GetCSharpSummary())
    public class @project.GetBuilder("Dtos").GetName(table.Name) : DtoBase<@table.PrimaryTypeName>
    {
        @foreach (var column in table.ColumnInfos)
        {
        @if (column.ColumnName == "Id")
         {
             continue;
         }
        @column.Comment.GetCSharpSummary()
        @Html.NewLine()
        @:public  @column.CsType @column.ColumnName { get; set; }
        @Html.NewLine()
        }
        @foreach (var navigate in table.NavigateInfos)
        {
        @navigate.Comment.GetCSharpSummary()
        @Html.NewLine()
        @:public virtual @project.GetBuilder("Dtos").GetName(navigate.NavigateTableInfo.Name) @navigate.ColumnName { get; set; }
        @Html.NewLine()
        }
    }
}