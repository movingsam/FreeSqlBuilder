@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using System.Collections.Generic;
using System.Text;

namespace @(project.ProjectInfo.ProjectName).Abstractes.Repositories
{

    public interface @(project.GetBuilder("IRepository").GetName(table.Name)) :IURepository<@(table.Name)@(",")I@(project.ProjectInfo.ProjectName)DbContext>
    {
    }
}
