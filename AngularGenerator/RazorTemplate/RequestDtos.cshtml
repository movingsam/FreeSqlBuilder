@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using @project.ProjectInfo.ProjectName@("."+table.Name).Dtos;
namespace @(project.ProjectInfo.ProjectName).@project.GetBuilder("RequestDtos").Name
{
    public class @project.GetBuilder("RequestDtos").GetName(table.Name) : RequestDtoBase
    {
    }
}