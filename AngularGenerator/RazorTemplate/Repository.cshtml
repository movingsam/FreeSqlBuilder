@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using System.Collections.Generic;
using System.Text;
using @(project.ProjectInfo.ProjectName).Abstractes.Repositories;

namespace @(project.ProjectInfo.ProjectName).Implaments.Repositories
{

@($"{(string.IsNullOrWhiteSpace(table.Comment)? table.Name :table.Comment)}仓储".GetCSharpSummary())
public class @(project.GetBuilder("Repository").GetName(table.Name)) : UowRepositoryBase<@(table.Name)@(",")@($"I{(project.ProjectInfo.ProjectName)}DbContext,{table.PrimaryTypeName}")>,@($"I{project.GetBuilder("Repository").GetName(table.Name)}")
    {
        @("构造函数".GetCSharpSummary())
        public @(project.GetBuilder("Repository").GetName(table.Name))(IServiceProvider service) : base(service)
        {
        }
    }
}