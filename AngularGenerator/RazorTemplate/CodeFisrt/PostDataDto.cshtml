@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using GRES.Framework.Dtos;
namespace @(project.ProjectInfo.ProjectName).@project.GetBuilder("PostDataDto").Name
{
    public class @project.GetBuilder("PostDataDto").GetName(table.Name) : PostDataDtoBase
    {
    @foreach (var column in table.ColumnInfos)
    {
        @if (column.ColumnName == "Id")
        {
            continue;
        }
        @column.Comment.GetCSharpSummary()
        @Html.NewLine()
        @column.GetAttribute()
        @:public  @column.CsType @column.ColumnName { get; set; }
        @Html.NewLine()
    }
    @foreach (var navigate in table.NavigateInfos)
    {
        @navigate.Comment.GetCSharpSummary()
        @Html.NewLine()
        @:public virtual @project.GetBuilder("PostDataDto").GetName(navigate.NavigateTableInfo.Name) @navigate.ColumnName { get; set; }
        @Html.NewLine()
    }
    }
    }