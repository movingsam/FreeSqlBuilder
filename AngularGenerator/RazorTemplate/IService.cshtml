@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using GRES.Framework.Services;

namespace @(project.ProjectInfo.ProjectName).Abstractes.@(project.GetBuilder("Service").Name))
{
    @($"{table.Comment}服务接口".GetCSharpSummary())
    public interface I@(project.GetBuilder("Service").GetName(table.Name)) : IServiceBase
    {

    }
}