@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using  @(project.ProjectName).Framework.Dtos;
namespace @(project.ProjectName).@project.GetBuilder("Dtos").Name
{
    public class @project.GetBuilder("Dtos").GetName(table.Name) : DtoBase<@(Model.FreeSql.DbFirst.GetCsType(table.Columns.FirstOrDefault(x=>x.IsPrimary)))>
    {
    @foreach (var column in table.Columns)
    {
        if(column.Name != "Id") { 
        @column.Coment.GetCSharpSummary()
        @Html.NewLine()
        <text>public virtual</text> <text>@(Model.FreeSql.DbFirst.GetCsType(column))</text> <text>@(project.Entity.Column.GetColumnName(column.Name))</text> <text>{ get; set; }</text>
        @Html.NewLine()
        }
    }
    }
}