@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using  @(project.ProjectName).Framework.Dtos;
namespace @(project.ProjectName).@project.GetBuilder("PostDataDto").Name
{
    public class @project.GetBuilder("PostDataDto").GetName(table.Name) : PostDataDtoBase
    {
    @foreach (var column in table.Columns)
    {
        if(column.Name != "Id") { 
        @column.Coment.GetCSharpSummary()
        @Html.NewLine()
        @:public virtual @(Model.FreeSql.DbFirst.GetCsType(column)) @(project.Entity.Column.GetColumnName(column.Name)) { get; set; }
        @Html.NewLine()
        }
    }
    }
}