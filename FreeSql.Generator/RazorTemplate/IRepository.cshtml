@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using System.Collections.Generic;
using System.Text;

namespace @(project.ProjectName).Abstractes.Repositories
{
    public interface I@(project.GetBuilder("Repository").GetName(table.Name)) :IURepository<@(project.Entity.GetName(table.Name))@(",")I@(project.ProjectName)DbContext>
    {
    }
}
