@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
    var dtoName =  project.GetBuilder("Dtos").GetName(table.Name);
    var primaryKeyType = Model.FreeSql.DbFirst.GetCsType(table.Columns.FirstOrDefault(x => x.IsPrimary));
}

using System;
using  @(project.ProjectName).Framework.Dtos;
namespace @(project.ProjectName).@project.GetBuilder("PageViewDto").Name
{
    public class @project.GetBuilder("PageViewDto").GetName(table.Name) : PageViewDtoBase<@($"{dtoName},{primaryKeyType}")>
    {
       public @(project.GetBuilder("PageViewDto").GetName(table.Name))(IPaged paged, List<@dtoName> datas):base(paged,datas)
       {
        
       }
    }
}