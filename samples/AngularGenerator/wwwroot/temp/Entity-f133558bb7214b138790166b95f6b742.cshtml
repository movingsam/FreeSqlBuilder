@using FreeSql.Generator.Core
@using FreeSql.TemplateEngine
@model FreeSql.TemplateEngine.BuildTask
@{
    Layout = "_Layout.cshtml";
    var project = Model.Project;
    var table = Model.CurrentTable;
}

using System;
using FreeSql.DataAnnotations;
using System.ComponentModel.DataAnnotations;
@table.GetNameSpaceUsing()

namespace @(project.ProjectInfo.ProjectName).@table.Name
{
    @table.Comment.GetCSharpSummary()
    public class @table.Name
    {
        @foreach (var column in table.ColumnInfos)
        {

            @column.Comment.GetCSharpSummary()
            @Html.NewLine()
            @column.GetAttribute()
            @:public virtual @(column.CsType) @column.ColumnName { get; set; }
            @Html.NewLine()
        }
    }
}